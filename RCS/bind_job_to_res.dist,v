desc
@bind a job to a specific reservation
@


1.3
log
@consistent opts
@
text
@#!___PYTHON_PATH_PLACEHOLDER___
# script to bind job to reservations

import Catalina
import getopt
import sys

options = ['res=', 'job=']
args = sys.argv[1:]
argdict = {}
optlist, lineargs = getopt.getopt(args, 'r:j', options)
for pair in optlist:
        argdict[pair[0]] = pair[1]

EVENTS_DB = 'events'
JOBS_DB = 'jobs'

events_db_handle = Catalina.open_db(EVENTS_DB,'write')
jobs_db_handle = Catalina.open_db(JOBS_DB,'write')

Catalina.bind_job_to_reservation(argdict['--job'], argdict['--res'], events_db_handle, jobs_db_handle)

Catalina.close_db(events_db_handle)
Catalina.close_db(jobs_db_handle)
@


1.2
log
@changed job_step_name to job_step_id
@
text
@d8 1
a8 1
options = ['reservation_name=', 'job_step_id=']
d21 1
a21 1
Catalina.bind_job_to_reservation(argdict['--job_step_id'], argdict['--reservation_name'], events_db_handle, jobs_db_handle)
@


1.1
log
@Initial revision
