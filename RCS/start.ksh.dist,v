count=`___PS_PLACEHOLDER___ -ef | ___GREP_PLACEHOLDER___ 'schedule_jobs' | ___GREP_PLACEHOLDER___ -v grep | ___WC_PLACEHOLDER___ -l | ___AWK_PLACEHOLDER___ '{print $1}'`
