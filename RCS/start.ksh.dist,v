desc
@script to start the scheduler
@


1.7
log
@took out newgrp
@
text
@#!___KSH_PATH_PLACEHOLDER___
cd ___HOMEDIR_PLACEHOLDER___
umask 002
./set_config --key_value=state:running
___NOHUP_PLACEHOLDER___ ./catalina_schedule_jobs --iterate >/dev/null 2>&1 &
count=`___PS_PLACEHOLDER___ ___PSOPTIONS_PLACEHOLDER___ | ___GREP_PLACEHOLDER___ 'catalina_schedule_jobs' | ___GREP_PLACEHOLDER___ -v grep | ___WC_PLACEHOLDER___ -l | ___AWK_PLACEHOLDER___ '{print $1}'`
while [[ $count < 1 ]]
do
___SLEEP_PLACEHOLDER___ 5
echo "waiting for start"
count=`___PS_PLACEHOLDER___ ___PSOPTIONS_PLACEHOLDER___ | ___GREP_PLACEHOLDER___ 'catalina_schedule_jobs' | ___GREP_PLACEHOLDER___ -v grep | ___WC_PLACEHOLDER___ -l | ___AWK_PLACEHOLDER___ '{print $1}'`
done
@


1.6
log
@dst tag -1
@
text
@a2 1
newgrp ___CATUSERGROUP_PLACEHOLDER___
@


1.5
log
@added PSOPTIONS
@
text
@d3 2
@


1.4
log
@use catalina_schedule_jobs instead of schedule_jobs
@
text
@d5 1
a5 1
count=`___PS_PLACEHOLDER___ -ef | ___GREP_PLACEHOLDER___ 'catalina_schedule_jobs' | ___GREP_PLACEHOLDER___ -v grep | ___WC_PLACEHOLDER___ -l | ___AWK_PLACEHOLDER___ '{print $1}'`
d10 1
a10 1
count=`___PS_PLACEHOLDER___ -ef | ___GREP_PLACEHOLDER___ 'catalina_schedule_jobs' | ___GREP_PLACEHOLDER___ -v grep | ___WC_PLACEHOLDER___ -l | ___AWK_PLACEHOLDER___ '{print $1}'`
@


